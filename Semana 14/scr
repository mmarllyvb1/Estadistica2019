Data = read.table(textConnection(Input),header=TRUE)
str(Data)
summarise(.data = group_by(Data, Var),
          count = n(),
          mean = mean(Azucarmg, na.rm = TRUE),
          sd = sd(Azucarmg, na.rm = TRUE))

# A tibble: 2 x 4
#Var      count  mean    sd
#<fct>    <int> <dbl> <dbl>
#1 Gulupa      28  18.0  14.8
#2 Maracuya    18  15.0  12.5


