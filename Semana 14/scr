#F1: 7
#F2: 8
#F3: 9
#F4: 4

##En Maracuya
#F1: 3 (+ 4) #Se necesitan 4
#F2: 6 (+ 2) #Se necesitan 2
#F3: 5 (+ 4) #Se necesitan 4
#F4: 4

##Se crean mas datos y se muestrean
F12 <- Data[which(Data$Fase== 'F1'),]
mean(F12$Azucarmg)
